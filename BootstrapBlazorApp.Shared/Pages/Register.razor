@page "/Register/{ReturnUrl?}"
@page "/Register/"


<ValidateForm class="form col-sm-4 align-self-center" Model="@RegisterDto" OnValidSubmit="OnValidSubmit">
    <div class="container">
        <div class="form-group col-12">
            <BootstrapInput @bind-Value="@RegisterDto.Account"></BootstrapInput>
        </div>
        <div class="form-group col-12">
            <BootstrapInput @bind-Value="@RegisterDto.Password"></BootstrapInput>
        </div>
        <div class="form-group col-12">
            <Button ButtonType="ButtonType.Submit" Text="注册" Color="Color.Primary"></Button>
        </div>
    </div>
</ValidateForm>